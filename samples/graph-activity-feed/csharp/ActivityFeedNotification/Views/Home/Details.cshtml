
@model IList<TabActivityFeed.Model.TaskInfo>
<table class="table">
    <tr>
        <th>
            Task Name
        </th>
        <th>
            Description
        </th>
        <th></th>
    </tr>

    @foreach (var item in TabActivityFeed.Repository.FeedRepository.Tasks)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.title)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.description)
            </td>
        </tr>
    }

</table>
